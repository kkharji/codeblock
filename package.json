{
  "name": "solid-codeblock",
  "version": "0.0.2",
  "description": "A lightweight library to render codeblock for solid-js",
  "main": "index.es.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "codeblock",
    "solidjs"
  ],
  "repository": {
    "url": "https://github.com/kkharji/solid-codeblock",
    "type": "git"
  },
  "author": "kkharji",
  "scripts": {
    "dev": "vite dev --config=vite.dev.ts",
    "build": "vite build --config=vite.prod.ts && tsc-alias && pnpm run copy",
    "copy": "cp package.json dist",
    "start": "vite --config=vite.dev.ts",
    "test": "jest --coverage",
    "linkShiki": "ln -sf $PWD/node_modules/shiki/* ./public/shiki"
  },
  "license": "MIT",
  "peerDependencies": {
    "shiki": "^0.11.1",
    "solid-js": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.17",
    "sass": "^1.55.0",
    "shiki": "^0.11.1",
    "solid-js": "^1.4.0",
    "tailwindcss": "^3.1.8",
    "tsc-alias": "^1.6.11",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-dts": "^1.2.0",
    "vite-plugin-solid": "^2.2.6",
    "vite-plugin-static-copy": "^0.9.0",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
